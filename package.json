{
  "name": "@tractorcow/beefdiff",
  "version": "1.5.1",
  "type": "module",
  "description": "Compare lockfile dependencies between two versions. Supports npm, composer, and pnpm lockfiles with detailed version change reports.",
  "bin": {
    "beefdiff": "./dist/bin.js"
  },
  "files": [
    "dist",
    "package.json"
  ],
  "scripts": {
    "build": "tsc",
    "postbuild": "chmod +x dist/bin.js",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm run clean && pnpm run build",
    "lint": "eslint .",
    "lint:commits": "commitlint --from HEAD~1 --to HEAD --verbose",
    "format": "prettier --write \"**/*.{ts,json,md}\"",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "test:coverage": "NODE_OPTIONS=--experimental-vm-modules jest --coverage",
    "test:coverage:open": "open coverage/lcov-report/index.html"
  },
  "keywords": [
    "lockfile",
    "dependencies",
    "diff",
    "npm",
    "composer",
    "pnpm",
    "package-manager",
    "version-comparison",
    "semver"
  ],
  "author": "Damian Mooyman",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@jest/globals": "^30.2.0",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^12.0.2",
    "@semantic-release/npm": "^13.1.2",
    "@semantic-release/release-notes-generator": "^14.1.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.1",
    "@types/semver": "^7.7.1",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^30.2.0",
    "prettier": "^3.7.1",
    "semantic-release": "^25.0.2",
    "ts-jest": "^29.4.5",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.0"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tractorcow/beefdiff.git"
  },
  "dependencies": {
    "@iarna/toml": "^2.2.5",
    "@yarnpkg/lockfile": "^1.1.0",
    "semver": "^7.7.3",
    "yaml": "^2.8.1"
  }
}
